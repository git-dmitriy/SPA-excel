{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parents": "off",
    "object-curly-spacing": ["error", "always"],
    "quotes": 0,
    "linebreak-style": 0,
    "require-jsdoc": 0
  },
  "extends": ["eslint:recommended", "google"]
}
