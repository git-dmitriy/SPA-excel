{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "arrow-parents": "off",
    "object-curly-spacing": ["error", "always"],
    "quotes": 0,
    "linebreak-style": 0,
    "require-jsdoc": 0,
    "operator-linebreak": "off",
    "indent": [2, 2]
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google"]
}
